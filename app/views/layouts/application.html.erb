<!DOCTYPE html>
<html>
<head>
    <title>Blogtask</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <% if user_signed_in? %>
    <%= render 'layouts/login'%>    
      <%= yield %>
    </div>
  <%else%>
    <%= render 'layouts/logout'%>
      <%= yield %>
    </div>
  <%end%>
</body>
</html>
